<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

       
        <!-- Payment Deposit sequences -->
        <record id="sequence_payment_deposit" model="ir.sequence">
            <field name="name">Payment Deposit Sequence</field>
            <field name="code">payment.deposit</field>
            <field name="prefix">DPT/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>
